{
    "name": "mtgofa/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": [
        "laravel",
        "shoppingcart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Tarek MTGofa",
            "email": "info@robMTGofa.nl"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/session": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~8.0 || ~9.0|| ~10.0 || ~11.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "MTGofa\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MTGofa\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "MTGofa/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MTGofa\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "MTGofa\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}